{
  "expo": {
    "name": "vibecode",
    "slug": "vibecode",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-sqlite",
      "expo-video",
      "expo-web-browser"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.dwang88.vibecode",
      "requireFullScreen": true,
      "deviceFamily": [1],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait"
        ],
        "UISupportedInterfaceOrientations~ipad": [],
        "NSCalendarsUsageDescription": "This app may use calendar access through third-party libraries for enhanced functionality.",
        "NSContactsUsageDescription": "This app may access contacts through third-party libraries for improved user experience.",
        "NSCameraUsageDescription": "This app may use camera access through third-party libraries for enhanced features."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.dwang88.vibecode"
    },
    "extra": {
      "eas": {
        "projectId": "e68dcc21-a162-410f-a579-0c01f6a475d2"
      }
    }
  }
}
